{
  "name": "sluggard",
  "license": "MIT",
  "description": "Sluggard is a bot for Discord using the DiscordPHP Library",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Devristo/phpws"
    }
  ],
  "require": {
    "php": "5.6.*",
    "dg/twitter-php": "3.*",
    "team-reflex/discord-php": "dev-develop",
    "react/datagram": "dev-master",
    "trafficcophp/bytebuffer": "dev-master",
    "ratchet/pawl": "dev-master",
    "monolog/monolog": "1.9.*",
    "nategood/commando": "0.2.*",
    "predis/predis": "1.0.*"
  },
  "autoload": {
    "psr-4": {
      "Sluggard\\": [
        "src/"
      ]
    }
  },
  "minimum-stability": "stable"
}
